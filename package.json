{
  "name": "stencil",
  "version": "0.1.1",
  "description": "A horribly simple boilerplate for frontend development",
  "main": "index.js",
  "scripts": {
    "install": "./scripts/install.js"
  },
  "author": "Richard Sentino",
  "license": "Apache",
  "private": true,
  "dependencies": {
    "express": "^4.10.4"
  },
  "browser": {
    "angular": "./node_modules/angular/angular.min.js",
    "angular-route": "./node_modules/angular-route/angular-route.min.js",
    "angular-sanitize": "./node_modules/angular-sanitize/angular-sanitize.min.js",
    "jQuery": "./node_modules/jquery/dist/jquery.min.js"
  },
  "browserify-shim": {
    "jQuery": "jQuery",
    "angular": {
      "depends": [
        "jQuery:jQuery"
      ],
      "exports": "angular"
    },
    "angular-route": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angular-route"
    },
    "angular-sanitize": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angular-sanitize"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "devDependencies": {
    "angular": "^1.3.5",
    "angular-route": "^1.3.5",
    "angular-sanitize": "^1.3.8",
    "bootstrap-styl": "^4.0.3",
    "browserify": "^7.0.0",
    "browserify-shim": "^3.8.3",
    "connect-livereload": "^0.5.2",
    "gulp": "^3.8.10",
    "gulp-angular-templatecache": "^1.4.2",
    "gulp-clean": "^0.3.1",
    "gulp-concat": "^2.4.2",
    "gulp-if": "^1.2.5",
    "gulp-livereload": "^2.1.1",
    "gulp-minify-css": "^1.0.0",
    "gulp-minify-html": "^0.1.7",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-stylus": "^2.0.1",
    "gulp-typescript": "^2.3.0",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.1",
    "gulp-vinyl-zip": "^1.0.7",
    "gulp-watch": "^3.0.0",
    "jquery": "^1.11.2",
    "run-sequence": "^1.0.2",
    "tiny-lr": "^0.1.4",
    "tsd": "^0.5.7",
    "typescript": "^1.3.0",
    "vinyl-source-stream": "^1.0.0",
    "vinyl-transform": "^1.0.0",
    "watchify": "^2.1.1",
    "yargs": "^3.7.1"
  }
}
